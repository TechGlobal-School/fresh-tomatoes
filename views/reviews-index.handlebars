<h1 class="text-center">All Reviews</h1>
<div class="row">
  {{#each reviews}}
    <div class="col-sm-12 col-md-6 col-lg-3 p-4 m-4 border border-dark">
      <h2><a href="/reviews/{{this._id}}">{{this.title}}</a></h2>
      <small>movie title: {{this.movieTitle}}</small>
    </div>
  {{/each}}
</div>